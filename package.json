{
  "name": "Palace",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack-cli -w",
    "start:electron": "npx electron .",
    "launch:electron": "npx npm-run-all -s build:electron:webpack start:electron",
    "build:electron:main": "npx npm-run-all -s build:electron:modules watch:electron:webpack",
    "b:e:w": "npx webpack-cli --config=./configs/webpack.electron.js",
    "watch:electron:webpack": "npx webpack-cli --config=webpack.electron.js -w",
    "build:electron:modules": "electron-rebuild -o keytar leveldown ursa",
    "build:cli": "npx webpack-cli --config=./configs/webpack.cli.js",
    "package": "npx electron-builder .",
    "cli": "node ./dist/cli.js"
  },
  "build": {
    "productName": "Palace",
    "appId": "io.dilta.palace",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "NSIS"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "chart.js": "^2.7.2",
    "date-fns": "^1.29.0",
    "file-loader": "^2.0.0",
    "global": "^4.3.2",
    "keypair": "^1.0.1",
    "keytar": "^4.3.0",
    "npm": "^6.4.0",
    "patternomaly": "^1.3.0",
    "pino": "^5.4.0",
    "pino-pretty": "^2.0.1",
    "pouchdb-adapter-leveldb": "^7.0.0",
    "rxdb": "^7.7.1",
    "uuid": "^3.3.2",
    "vue": "^2.5.2",
    "vue-chartjs": "^3.4.0",
    "vue-class-component": "^6.0.0",
    "vue-notification": "^1.3.13",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.2.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.35",
    "@types/inquirer": "^0.0.43",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.2",
    "@types/uuid": "^3.4.3",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "commander": "^2.19.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.29.0",
    "electron-rebuild": "^1.8.2",
    "faker": "^4.1.0",
    "happypack": "^5.0.0",
    "inquirer": "^6.2.0",
    "material-design-icons-iconfont": "^3.0.3",
    "node-rsa": "^1.0.1",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "readline": "^1.3.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "2.7",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "description": "",
  "keywords": [],
  "author": {
    "name": "Abiodun Avenuer",
    "url": "https://github.com/avenuer"
  },
  "license": "Apache-2.0"
}
